ImportPackage("conio")
ImportPackage("system")
ImportPackage("windows")
ImportPackage("math")
SetConsoleTitle("GIT")
System("@git status -s")
Println("Pushing R11")
System("@date /t")
System("@time /t")
System("@type version.ver")
Print("Commit Message: ")
local input = GetInput()
System("@git add *")
System("@git commit -m "..tostring(input))
Pause()

